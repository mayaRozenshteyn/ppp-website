<%- include("partials/header") %>
<form id="contact-form" class="w-100">
  <div class="form-group col-9 col-md-3">
    <label for="exampleFormControlInput1">First & Last Name</label>
    <input type="text" class="form-control" id="exampleFormControlInput1" placeholder="Mark Frerichs" />
  </div>

  <div class="form-group col-9 col-md-3">
    <label for="exampleFormControlInput1">Email address</label>
    <input type="email" class="form-control" id="exampleFormControlInput2" placeholder="name@gmail.com" />
  </div>
  <div class="form-group col-9 col-md-3">
    <label for="exampleFormControlSelect1">Example select</label>
    <select class="form-control" id="exampleFormControlSelect1">
      <option class="text-secondary">[select topic]</option>
      <option>Content</option>
      <option>Feedback</option>
      <option>Partnership</option>
      <option>Request Materials</option>
      <option>Other</option>
    </select>
  </div>

  <div class="form-group col-9 col-lg-5">
    <label for="exampleFormControlTextarea1">Type your message below :)</label>
    <textarea class="form-control" id="message-box" rows="3"></textarea>
  </div>

  <div class="form-group">
    <div class="col-sm-10">
      <button type="submit" class="btn btn-primary">Send Message</button>
    </div>
  </div>
</form>
<%- include("partials/footer") %>